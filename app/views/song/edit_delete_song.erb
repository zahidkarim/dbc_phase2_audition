<% if song.user_id == current_user.id %>
  <form action="/songs/<%= song.id %>" method="POST">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete This Song">
  </form>

  <form action="/songs/<%= song.id %>" method="POST">
  <input type="hidden" name="_method" value="PUT">
  Edit -- Title:
  <input name="song[title]" type="text" value="<%= song.title %>">
  Album Art:
  <input name="song[album_art]" type="text" value="<%= song.album_art %>">
  Song URL:
  <input name="song[song_url]" type="text" value="<%= song.song_url %>">
  <input type="submit" value="Edit this song"><br>
  </form>
<% else %>
<% end %>